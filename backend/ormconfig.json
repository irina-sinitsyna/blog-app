{
  "type": "postgres",
  "host": "${DB_HOST}",
  "port": "${DB_PORT}",
  "username": "${DB_USERNAME}",
  "password": "${DB_PASSWORD}",
  "database": "${DB_DATABASE}",
  "synchronize": "${DB_SYNCHRONIZE}",
  "logging": "${DB_LOGGING}",
  "entities": ["dist/**/*.entity{.ts,.js}"],
  "migrations": ["dist/migration/**/*.js"],
  "cli": {
    "migrationsDir": "src/migration"
  }
}
